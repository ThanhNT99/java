<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>AuthenticationFilter</filter-name>
        <filter-class>Filter.LoginFilter</filter-class>
    </filter>
    <servlet>
        <servlet-name>SignUpController</servlet-name>
        <servlet-class>authentication.SignUpController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>authentication.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteController</servlet-name>
        <servlet-class>entity.admin.DeleteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertController</servlet-name>
        <servlet-class>entity.admin.InsertController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateController</servlet-name>
        <servlet-class>entity.admin.UpdateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListAdminController</servlet-name>
        <servlet-class>entity.admin.ListAdminController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchTestController</servlet-name>
        <servlet-class>entity.admin.SearchTestController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchQuestionController</servlet-name>
        <servlet-class>entity.admin.SearchQuestionController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewQuestion</servlet-name>
        <servlet-class>entity.admin.ViewQuestion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadFileController</servlet-name>
        <servlet-class>entity.admin.UploadFileController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddTestSuccess</servlet-name>
        <servlet-class>entity.admin.AddTestSuccess</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doTestController</servlet-name>
        <servlet-class>entity.client.DoTestController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>listClient</servlet-name>
        <servlet-class>entity.client.ListAllTest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientTestList</servlet-name>
        <servlet-class>entity.client.ClientTestList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Client_ViewQuestion</servlet-name>
        <servlet-class>entity.client.ClientViewQuestion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>submitTest</servlet-name>
        <servlet-class>entity.client.SubmitTest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeForward</servlet-name>
        <servlet-class>Home.HomeForward</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOutController</servlet-name>
        <servlet-class>authentication.LogOutController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignUpController</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteController</servlet-name>
        <url-pattern>/entityAdmin/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertController</servlet-name>
        <url-pattern>/entityAdmin/insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateController</servlet-name>
        <url-pattern>/entityAdmin/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListAdminController</servlet-name>
        <url-pattern>/entityAdmin/listadmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchTestController</servlet-name>
        <url-pattern>/entityAdmin/searchTest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchQuestionController</servlet-name>
        <url-pattern>/entityAdmin/searchQuestion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewQuestion</servlet-name>
        <url-pattern>/entityAdmin/viewQuestion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadFileController</servlet-name>
        <url-pattern>/entityAdmin/uploadFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddTestSuccess</servlet-name>
        <url-pattern>/entityAdmin/AddTestSuccess</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doTestController</servlet-name>
        <url-pattern>/entityClient/doTest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>listClient</servlet-name>
        <url-pattern>/entityClient/listAllTest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientTestList</servlet-name>
        <url-pattern>/entityClient/clientTestList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Client_ViewQuestion</servlet-name>
        <url-pattern>/entityClient/viewQuestion_Client</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>submitTest</servlet-name>
        <url-pattern>/entityClient/submittest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeForward</servlet-name>
        <url-pattern>/homeForward</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOutController</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>
            /business-2/home.html
        </welcome-file>
    </welcome-file-list>
</web-app>
